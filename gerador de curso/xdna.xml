    <item>
      <title>O X-DNA® é indicado principalmente para:</title>
      <dc:creator><![CDATA[dra-ana-cleia-barbosa]]></dc:creator>
      <content:encoded><![CDATA[]]></content:encoded>
      <wp:post_id>100006</wp:post_id>
      <wp:status>publish</wp:status>
      <wp:post_type>lp_question</wp:post_type>
      <wp:post_name>o-x-dna-e-indicado-principalmente-para</wp:post_name>
      <wp:answer>
        <wp:question_id>100006</wp:question_id>
        <wp:answer_title><![CDATA[Peles maduras com sinais visíveis de envelhecimento e danos celulares]]></wp:answer_title>
        <wp:answer_value>val_100006_1</wp:answer_value>
        <wp:answer_order>1</wp:answer_order>
        <wp:answer_is_true>yes</wp:answer_is_true>
      </wp:answer>
      <wp:answer>
        <wp:question_id>100006</wp:question_id>
        <wp:answer_title><![CDATA[Peles extremamente jovens sem necessidade de revitalização]]></wp:answer_title>
        <wp:answer_value>val_100006_2</wp:answer_value>
        <wp:answer_order>2</wp:answer_order>
        <wp:answer_is_true></wp:answer_is_true>
      </wp:answer>
      <wp:answer>
        <wp:question_id>100006</wp:question_id>
        <wp:answer_title><![CDATA[Exclusivamente para uso capilar]]></wp:answer_title>
        <wp:answer_value>val_100006_3</wp:answer_value>
        <wp:answer_order>3</wp:answer_order>
        <wp:answer_is_true></wp:answer_is_true>
