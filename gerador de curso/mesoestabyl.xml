    <item>
      <title>A “Eliminação e aproveitamento energético”, os produtos finais da lipólise são principalmente:</title>
      <dc:creator><![CDATA[dra-ana-cleia-barbosa]]></dc:creator>
      <content:encoded><![CDATA[]]></content:encoded>
      <wp:post_id>100003</wp:post_id>
      <wp:status>publish</wp:status>
      <wp:post_type>lp_question</wp:post_type>
      <wp:post_name>a-eliminacao-e-aproveitamento-energetico-os-produtos-finais-da-lipolise-sao-principalmente</wp:post_name>
      <wp:answer>
        <wp:question_id>100003</wp:question_id>
        <wp:answer_title><![CDATA[Lactato e piruvato]]></wp:answer_title>
        <wp:answer_value>val_100003_1</wp:answer_value>
        <wp:answer_order>1</wp:answer_order>
        <wp:answer_is_true></wp:answer_is_true>
      </wp:answer>
      <wp:answer>
        <wp:question_id>100003</wp:question_id>
        <wp:answer_title><![CDATA[Ácidos graxos livres e glicerol]]></wp:answer_title>
