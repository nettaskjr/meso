      <content:encoded><![CDATA[]]></content:encoded>
      <wp:post_id>100003</wp:post_id>
      <wp:status>publish</wp:status>
      <wp:post_type>lp_question</wp:post_type>
      <wp:post_name>em-cicatrizes-atroficas-profundas-qual-abordagem-tecnica-e-mais-apropriada</wp:post_name>
      <wp:answer>
        <wp:question_id>100003</wp:question_id>
        <wp:answer_title><![CDATA[Profundidade única em toda a área]]></wp:answer_title>
        <wp:answer_value>val_100003_1</wp:answer_value>
        <wp:answer_order>1</wp:answer_order>
        <wp:answer_is_true></wp:answer_is_true>
      </wp:answer>
      <wp:answer>
        <wp:question_id>100003</wp:question_id>
        <wp:answer_title><![CDATA[Técnica cruzada com diferentes profundidades (>1 mm)]]></wp:answer_title>
